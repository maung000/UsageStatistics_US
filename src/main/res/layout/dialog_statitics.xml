<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e3fdf9">
    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="THỐNG KÊ THỜI GIAN SỬ DỤNG ỨNG DỤNG"
        android:textSize="25sp"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="#000000"
        android:id="@+id/textView" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="80dp"
        android:layout_height="150dp"
        android:layout_marginLeft="120dp"
        android:layout_marginRight="120dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/statitics" />

    <TableRow
        android:layout_marginTop="20dp"
        android:layout_marginLeft="130dp">

        <TextView

            android:id="@+id/name_dayStart"
            android:layout_width="184dp"
            android:layout_height="wrap_content"
            android:text="Thời gian bắt đầu:"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />
        <EditText
            android:id="@+id/dayStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#e3fdf9"
            android:textColor="#000000"
            android:hint=" Chọn thời gian " />


    </TableRow>
    <TableRow
        android:layout_marginTop="20dp"
        android:layout_marginLeft="130dp">

        <TextView
            android:id="@+id/name_dayEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Thời gian kết thúc:"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/dayEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#e3fdf9"
            android:textColor="#000000"
            android:hint=" Chọn thời gian " />

    </TableRow>

    <Button
        android:id="@+id/btn_statitic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="20dp"
        android:text="Chạy thống kê" />


    <TableRow
        android:layout_marginTop="20dp"
        android:layout_marginLeft="130dp"
        android:layout_marginBottom="30dp">

        <TextView
            android:id="@+id/name_sum_time"
            android:layout_width="135dp"
            android:layout_height="wrap_content"
            android:text="Tổng thời gian:"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/sum_time"
            android:layout_width="64dp"
            android:layout_height="wrap_content"
            android:width="10dp"
            android:height="30dp"
            android:background="#e3fdf9"
            android:textColor="#000000"/>
    </TableRow>



</TableLayout>
